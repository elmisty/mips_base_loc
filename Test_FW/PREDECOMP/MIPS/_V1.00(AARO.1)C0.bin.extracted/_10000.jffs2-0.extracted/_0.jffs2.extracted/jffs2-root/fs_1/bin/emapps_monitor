#!/bin/sh

## Emapps monitor
	local emappS_pid
	system_date=$(date)
	# check EmappS daemon			
	emappS_pid=$(ps | grep "/usr/bin/EmappS" | grep "grep" -v | awk '{print $1}')
		
	if [ "$emappS_pid" == "" ] ;then
		echo "$system_date  Emapps_die" >> /tmp/emapps_monitor_log			
		/usr/bin/EmappS &		
	fi	
