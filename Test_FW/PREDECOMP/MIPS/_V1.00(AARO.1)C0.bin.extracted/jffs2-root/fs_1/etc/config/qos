# This is area for porting Atheros by benny ----start

config qos_general general
	option enable 0
	option game_enable 0	

config qos shaper
	option port_rate_eth0		819200
	option port_rate_lan		819200

config qos_priority priority
	option game "6"
	option voip "5"
	option media "4"
	option web "3"
	option ftp "2"
	option mail "1"

config qos_egress_policy app_policy_0
	option prio 1
	option enable 0
	option cat "Game Console"
	option serv "XBox Live"
	option tcp_port 3074,3390,3932,5555
	option udp_port 88,1900,3074,3776,3777
	option lan_tcp_enable 1
	option lan_udp_enable 1
	option wan_tcp_enable 1
	option wan_udp_enable 1	
	option lan_tcp_min 1
	option lan_udp_min 1
	option wan_tcp_min 1
	option wan_udp_min 1
	option lan_tcp_bw 50
	option lan_udp_bw 50
	option wan_tcp_bw 10
	option wan_udp_bw 10	
	
