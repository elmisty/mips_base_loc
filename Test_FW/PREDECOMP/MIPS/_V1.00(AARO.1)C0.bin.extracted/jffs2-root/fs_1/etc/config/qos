config qos_user_egress_policy eg_policy_1
	option enable 0
	option name ""
	option reserve_bw 2
	option bw_value 10
	option inipaddr_start "0.0.0.0"
	option inipaddr_end "0.0.0.0"
	option inport 0
	option outipaddr_start "0.0.0.0"
	option outipaddr_end "0.0.0.0"
	option outport 0
	option proto ""
	option to_intf lan
	option apptype "Game Console"

