20<@>function IP2Num(ip)
2<@>{
17<@>    var num = 0;
24<@>    ip = ip.split(".");
109<@>    num = Number(ip[0]) * 256 * 256 * 256 + Number(ip[1]) * 256 * 256 + Number(ip[2]) * 256 + Number(ip[3]);
21<@>    num = num >>> 0;
16<@>    return num;
2<@>}
